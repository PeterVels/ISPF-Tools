/* Rexx to perform simple file tailoring */
/* address TSO "ISPFTTRC" */  /* start  trace */
arg skel pgmname /* PGMNAME is used in UNITTEST skel */
address ISPEXEC
"ftopen temp"
"ftincl" skel "EXT"
"ftclose"
"vget (ztempn) shared"
"LMINIT datAID(tempID) DDNAME("ztempn")"
"edit dataID("tempID")"
"LMFREE dataID("tempID")"
/* address TSO "ISPFTTRC" */ /* end and display trace */
exit
